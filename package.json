{
  "name": "sandbox",
  "version": "1.0.0",
  "description": "Application for solving js tasks",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build-prod": "npm run build --prefix client && rm -rf public/* && ncp client/dist public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codyfet/sandbox.git"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "semantic-ui-react",
    "mongodb",
    "codermirror",
    "react-codemirror2"
  ],
  "author": "codyfet <codyfet@mail.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codyfet/sandbox/issues"
  },
  "homepage": "https://github.com/codyfet/sandbox#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mochawesome": "^6.1.1",
    "mongoose": "^5.9.14",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
