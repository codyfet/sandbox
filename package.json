{
  "name": "sandbox",
  "version": "1.0.0",
  "description": "Application sandbox for solving js tasks.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-prebuild": "npm install --dev",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codyfet/sandbox.git"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "semantic-ui-react",
    "mongodb",
    "codermirror",
    "react-codemirror2",
    "mocha"
  ],
  "author": "codyfet <codyfet@mail.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codyfet/sandbox/issues"
  },
  "homepage": "https://github.com/codyfet/sandbox#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mochawesome": "^6.1.1",
    "mongoose": "^5.12.8"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": "12.x"
  }
}
